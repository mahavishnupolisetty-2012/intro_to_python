   ],
   "source": [
    "m = int(input(\"enter the number: \"))\n",
    "fact=0\n",
    "for number in range (1,m+1):\n",
    "    if number%2 != 0:\n",
    "        fact = fact +number\n",
    "print(\"sum of odd numbers for the entered number is: \")\n",
    "print(fact)"
